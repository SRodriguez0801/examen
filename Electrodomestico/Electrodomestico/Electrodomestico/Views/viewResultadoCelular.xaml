<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Electrodomestico.Views.viewResultadoCelular"
             xmlns:local="clr-namespace:Electrodomestico.ViewModels" >
    <ContentPage.BindingContext>
        <local:ViewModelsResultadoCelular></local:ViewModelsResultadoCelular>
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" Padding="30">
            <Label Text="{Binding ResultadoCelular.Nombre}" FontSize="18" TextColor="Black" Margin="0,0,0,10"/>
            <Label Text="{Binding ResultadoCelular.Modelo}" FontSize="18" TextColor="Black" Margin="0,0,0,10"/>
            <Label Text="{Binding ResultadoCelular.FechaLanzamiento}" FontSize="18" TextColor="Black" Margin="0,0,0,10"/>
            <Label Text="{Binding ResultadoCelular.Voltaje}" FontSize="18" TextColor="Black" Margin="0,0,0,10"/>
            <Label Text="{Binding ResultadoCelular.PrecioCompra}" FontSize="18" TextColor="Black" Margin="0,0,0,10"/>
            <Label Text="{Binding ResultadoCelular.SistemaOperativo}" FontSize="18" TextColor="Black" Margin="0,0,0,10"/>
            <Label Text="{Binding ResultadoCelular.OperadorRed}" FontSize="18" TextColor="Black" Margin="0,0,0,10"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>